{
  "name": "kashir-example",
  "version": "0.0.19",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "check": "yarn typecheck && yarn lint && yarn format",
    "typecheck": "npx tsc --noEmit",
    "lint": "npx eslint .",
    "format": "npx prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "build:android": "react-native build-android --extra-params \"--no-daemon --console=plain\"",
    "build:android:release": "react-native build-android --mode Release --extra-params \"--no-daemon --console=plain\"",
    "build:ios": "react-native build-ios --scheme Kashir --mode Debug --extra-params \"-sdk iphonesimulator CC=clang CPLUSPLUS=clang++ LD=clang LDPLUSPLUS=clang++ GCC_OPTIMIZATION_LEVEL=0 GCC_PRECOMPILE_PREFIX_HEADER=YES ASSETCATALOG_COMPILER_OPTIMIZATION=time DEBUG_INFORMATION_FORMAT=dwarf COMPILER_INDEX_STORE_ENABLE=NO\""
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-clipboard/clipboard": "^1.16.3",
    "@react-native-community/datetimepicker": "^8.4.4",
    "@react-native-community/geolocation": "^3.4.0",
    "@react-navigation/native": "^7.1.16",
    "@react-navigation/native-stack": "^7.3.23",
    "ngeohash": "^0.6.3",
    "react": "19.0.0",
    "react-native": "0.78.1",
    "react-native-camera-kit": "^15.1.0",
    "react-native-confetti-cannon": "^1.5.2",
    "react-native-config": "^1.5.5",
    "react-native-fs": "^2.20.0",
    "react-native-keychain": "^10.0.0",
    "react-native-maps": "^1.24.13",
    "react-native-permissions": "^5.4.1",
    "react-native-qrcode-svg": "^6.3.2",
    "react-native-safe-area-context": "^5.5.2",
    "react-native-screens": "^4.13.1",
    "react-native-svg": "^15.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "15.0.1",
    "@react-native-community/cli-platform-android": "15.0.1",
    "@react-native-community/cli-platform-ios": "15.0.1",
    "@react-native/babel-preset": "0.78.1",
    "@react-native/metro-config": "0.78.1",
    "@react-native/typescript-config": "0.78.1",
    "@types/ngeohash": "^0.6.8",
    "@types/react": "^19.0.0",
    "react-native-builder-bob": "^0.38.3"
  },
  "engines": {
    "node": ">=18"
  }
}
